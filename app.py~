from flask import Flask, render_template
occupations = Flask(__name__)##creates an instance of a flask and instatiates its name

@occupations.route("/occupations")

def hello_world():
    return "No hablo queso"

coll = [1, 3, 3, 7]

@occupations.route("/my_first_template")
def tet_template():
    return render_template("foo.html", foo = "SILENCIO", fool=coll)

    
if __name__=="__main__":
        occupations.debug = True
        occupations.run()
        
